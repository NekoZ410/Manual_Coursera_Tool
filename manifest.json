{
    "manifest_version": 3,
    "name": "Manual Coursera Tool",
    "version": "1.1.0",
    "description": "Manual tool to complete Coursera quizzes when your other automation tools not working :)))))",
    "permissions": ["activeTab", "scripting", "storage"],
    "homepage_url": "https://github.com/NekoZ410/Manual_Coursera_Tool",
    "host_permissions": ["https://www.coursera.org/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {}
    },
    "content_scripts": [
        {
            "matches": ["https://www.coursera.org/*"],
            "js": ["content.js"]
        }
    ]
}
