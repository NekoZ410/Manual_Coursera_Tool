{
    "manifest_version": 3,
    "name": "coursera-manual-extension",
    "version": "1.0.0",
    "description": "Manual tool to complete Coursera when your other automation tools not working :)))))",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {}
    },
    "content_scripts": [
        {
            "matches": ["https://www.coursera.org/*"],
            "js": ["content.js"]
        }
    ]
}
